<h1>All Order</h1>
<ul>
   
    <% @orders.each do |m| %>
        <li>
            <a href="/orders/<%= m.id %>">
                <%= m.address %>
                <%= m.item %>
                <%= m.item_price%>
                <%= m.quantity %>
                <%= m.total %>   
            </a>
        </li>
    <% end %>
   
    <a href="/orders/new" class="button">Add Items</a>
</ul>

